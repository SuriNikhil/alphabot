<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>BOT API</display-name>
    <listener>
        <listener-class>servlets.ContextManager</listener-class>
    </listener>
    <servlet>
        <servlet-name>QueryResponseServlet</servlet-name>
        <servlet-class>servlets.QueryResponseServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>QueryResponseServlet</servlet-name>
        <url-pattern>/queryResponseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryResponseServlet</servlet-name>
        <url-pattern>/QueryResponseServlet</url-pattern>
    </servlet-mapping>
</web-app>

